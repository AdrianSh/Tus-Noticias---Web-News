create table actividad (id bigint generated by default as identity (start with 1), estado varchar(255), user bigint) 
create table articulo (id bigint generated by default as identity (start with 1), fecha timestamp, ranking integer not null, titulo varchar(255), autor bigint)
create table articulo_contenido (articulo bigint not null, contenido varchar(255))
create table articulo_puntuaciones_id (articulo bigint not null, puntuaciones_id integer)
create table comentario (id bigint generated by default as identity (start with 1), comment varchar(255), articulo bigint, responde bigint, user bigint, receptor bigint)
create table comentario_puntuaciones_id (comentario bigint not null, puntuaciones_id integer)
create table comentario_respuestas (comentario bigint not null, respuestas bigint not null)
create table puntuacion (id bigint generated by default as identity (start with 1), negativos integer not null, positivos integer not null)
create table tag (nombre varchar(255) not null)
create table tag_articulo (tags varchar(255) not null, articulo bigint not null)
create table user (id bigint generated by default as identity (start with 1), hashed_and_salted varchar(255), login varchar(255), role varchar(255))
create table user_favoritos (user bigint not null, favoritos bigint not null)
create table user_puntuaciones_id (user bigint not null, puntuaciones_id integer)
create table user_seguidores (user bigint not null, seguidores bigint not null)
create table user_seguidos (user bigint not null, seguidos bigint not null)

Orden por el que hay que ir metiendo los datos en el import.sql .

actividad(id , estado , user)
articulo (id , fecha, ranking , titulo , autor)
articulo_contenido(articulo,contenido)
articulo_puntuaiones_id(articulo , puntuaciones_id)
comentario(id , comment , articulo , responde,user , receptor)
comentario_puntuaciones_id (comentario , puntuaciones_id)
comentario_respuestas(comentario,respuestas)
puntuacion(id,negativos,positivos);